        <!--</File>-->
    </Appenders>

    <Loggers>
        <!--<Logger name="mh.sample2.Log4jTest2" level="INFO">-->
            <!--<AppenderRef ref="File" />-->
        <!--</Logger>-->
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>